{
  "name": "lego-react-antd",
  "version": "0.1.0",
  "description": "lego board.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "dev": "npm run build & npm start",
    "lint": "eslint app front *.js",
    "build": "webpack --config ./build/webpack.config.dev.js --progress -w",
    "build:dll": "webpack --config ./build/webpack.config.dll.js --progress",
    "build:prod": "npm run build:dll && webpack --config ./build/webpack.config.prod.js -p"
  },
  "keywords": [
    "node.js",
    "koa",
    "react",
    "mobx"
  ],
  "author": "tvrcgo",
  "license": "MIT",
  "dependencies": {
    "lego": "github:tvrcgo/lego",
    "lego-render": "github:lego-component/render",
    "lego-static": "github:lego-component/static"
  },
  "devDependencies": {
    "@types/react": "^15.0.0",
    "antd": "^2.6.1",
    "assets-webpack-plugin": "^3.5.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "lego-livereload": "github:lego-component/livereload",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.0",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.5",
    "webpack": "^1.13.1"
  }
}
